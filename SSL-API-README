curl -i http://neutron-controller-hostname:9696/v2.0/lb/ssl_certificate_keys.json -X GET -H "X-Auth-Token: f80dd2bd2b2d4a898b713068c5c4b6e2" -H "Content-Type: application/json" -H "Accept: application/json" -H "User-Agent: python-neutronclient"

